<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-/mybatis.org//DTD
Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baizhi.dao.LogDAO">
    <insert id="insertOne" parameterType="Log">
        insert  into f_log values(#{id},#{person},#{date},#{thing},#{success})
    </insert>
    <select id="queryAll" resultType="Log">
        select * from f_log
    </select>
</mapper>